let pizzaToppings = [pepperoni, sausage, onions, peppers];
function greetCustomer() {
  console.log("Welcome to Big Pizza, our toppings are:");
}
greetCustomer();
for (let topping of pizzaToppings){
  return topping;
}
let size = "large";
let crust = "thin";
function getPizzaOrder(large, thin, ...topping){
  console.log("One pizza coming up!");
  return(size, crust, topping);
}
function preparePizza ([large, thin, pepperoni, peppers]){
  return "Cooking pizza." + {size: "large", crust: "thin", topping: "pepperoni" + " " $(peppers)};
} function servePizza ({size: medium, crust: thin, toppings: pepperoni} ){
  return ("Pizza is ready!" {size: "large", crust: "thin", toppings: `pepperonipeppers.`);
}
function greetCustomer();
function getPizzaOrder();
function preparePizza(getPizzaOrder);
function servePizza(preparePizza);
