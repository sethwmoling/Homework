let pizzaToppings = [pepperoni, sausage, onions, peppers]
function greetCustomer() {
  for (let toppings of pizzaToppings)
} return “Welcome to Big Pizza, our (toppings) are (pizzaToppings)“
function getPizzaOrder (size, crust, ...toppings) {
  console.log("One large thick crust pizza with (toppings) ... coming up!")
} function preparePizza ([large, thin crust, pepperoni, peppers]){
  return ("Cooking pizza." + {size: large, crust: thin, toppings: pepperoni + peppers })
} function servePizza ({size: medium, crust: thin, toppings: pepperoni} ){
  return ("Pizza is ready! Large, thin crust with pepperoni and peppers + {size: large, crust: thin, toppings: pepperoni + peppers.")
}
function greetCustomer()
function getPizzaOrder()
function preparePizza()
function servePizza()
